#include <iostream>
#include <cmath>

using namespace std;

int main() {
    setlocale(LC_ALL, "rus");
    double a, xn, xk, dx;
    cout << "enter a" << endl;
    cin >> a;
    cout << "enter xn" << endl;
    cin >> xn;
    cout << "enter xk" << endl;
    cin >> xk;
    cout << "enter dx" << endl;
    cin >> dx;


    double sum_y = 0;
    double x = xn;
    int i = 0;

    while (x <= xk) {
        double y = (pow((a * x), 1 / 3)) / (5 + cos(a) + exp(x));

       if (i % 2== 0) {
            cout << "x = " << x << ", y = " << y << endl;
        }

        sum_y += y;
        x += dx;
        i++;
    }

    cout << "Сумма значений y: " << sum_y << endl;



    return 0;
}
